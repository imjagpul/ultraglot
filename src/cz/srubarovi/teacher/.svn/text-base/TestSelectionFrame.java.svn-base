    QuestionFrame frame;
    private void jButtonRunActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonRunActionPerformed
        Test test=null;
        
        CheckNode root=(CheckNode) jTreeTestSelection.getModel().getRoot();
        ArrayList<DataTest> selectedTest=new ArrayList<DataTest>();
        ArrayList<String> pathnames=new ArrayList<String>();
        
        for (Enumeration en=root.preorderEnumeration(); en.hasMoreElements();) {
            CheckNode node = (CheckNode) en.nextElement();
            if(node.isLeaf() && node.isSelected()) {
                NodeEntry entry=(NodeEntry) node.getUserObject();
                try {
                    selectedTest.add(entry.getType().loadTest(entry));
                    pathnames.add(entry.getPathname());
                } catch (TestFileFormatException ex) {
                    JOptionPane.showMessageDialog(this, "Unkown file format:"+entry.toString());
                } catch (IOException ex) {
                    JOptionPane.showMessageDialog(this, "IO exception:"+entry.toString());
                }
            }
        }
        
        if(selectedTest.size()==0) {
            JOptionPane.showMessageDialog(this, "První zaškrtni testy, které chceš spustit.");
            return;
        }
        
        if(!jCheckBoxCollectStats.isSelected()) {
            
            test=new TestJoin(selectedTest);
            
            if(jCheckBoxShuffle.isSelected()) test=new TestShuffle(test);
            if(jCheckBoxReverse.isSelected()) test=new TestReverse(test);
            if(jCheckBoxSmart.isSelected()) test=new SmartTest(test);
        } else {
            
            assert selectedTest.size()>0; //uz to bylo testovano
            assert pathnames.size()==selectedTest.size();
            
            DataTest[] dataTestArray=new DataTest[selectedTest.size()];
            String[] pathnamesArray=new String[dataTestArray.length];
            
            dataTestArray=selectedTest.toArray(dataTestArray);
            pathnamesArray=pathnames.toArray(pathnamesArray);
            
            Strategy strategy=(Strategy) jComboBoxStrategy.getSelectedItem();            
            test=StatsTest.load(dataTestArray, pathnamesArray, jCheckBoxReverse.isSelected(), strategy);
        }
        
        
        if(frame==null) {
            frame=new QuestionFrame();
        }
        
        frame.setTestAccents(jCheckBoxAccents.isSelected());
        frame.setTestWordTypes(jCheckBoxWordTypes.isSelected());
        frame.startTest(test);
    }//GEN-LAST:event_jButtonRunActionPerformed

    private StatsBrowser statsBrowser;
    private void jMenuItemBrowseStatsActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jMenuItemBrowseStatsActionPerformed
        makeSureLoggedIn();
        
        if (statsBrowser == null) {
            statsBrowser = new StatsBrowser();
        }

        CheckNode root = (CheckNode) jTreeTestSelection.getModel().getRoot();
        ArrayList<String> pathnames = new ArrayList<String>();

        for (Enumeration en = root.preorderEnumeration(); en.hasMoreElements();) {
            CheckNode node = (CheckNode) en.nextElement();
            if (node.isLeaf() && node.isSelected()) {
                NodeEntry entry = (NodeEntry) node.getUserObject();
                pathnames.add(entry.getPathname());
            }
        }

        if(pathnames.size()==0) {
            JOptionPane.showMessageDialog(this, "První zaškrtni testy, jejihž statistiku chceš prozkoumat.");
            return;
        }
        
        statsBrowser.setPathnames(pathnames);
        statsBrowser.setVisible(true);

    }//GEN-LAST:event_jMenuItemBrowseStatsActionPerformed

