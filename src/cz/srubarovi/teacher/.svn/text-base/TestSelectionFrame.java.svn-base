    private void updateEnabledState() {
        boolean collectStats=jCheckBoxCollectStats.isSelected();
        jComboBoxStrategy.setEnabled(collectStats);        
        jCheckBoxShuffle.setEnabled(!collectStats);
        jCheckBoxSmart.setEnabled(!collectStats);
    }
    
    private void makeSureLoggedIn() {
        initStatsDir();
        
        if(Settings.INSTANCE.getStatsProfile()!=null)
            return;
        
        int count=ProfileManager.getProfileCount();
        if(count==0) {
            String newProfile=JOptionPane.showInputDialog(this, "Enter new profile name.", "default");
            Settings.INSTANCE.setStatsProfile(newProfile);
        } else if(count==1) {
            Settings.INSTANCE.setStatsProfile(ProfileManager.listProfiles()[0]);
        } else {
            Settings.INSTANCE.setStatsProfile(ProfileManager.listProfiles()[0]);
            profileManagerFrame.setVisible(true);
        }        
    }
    
