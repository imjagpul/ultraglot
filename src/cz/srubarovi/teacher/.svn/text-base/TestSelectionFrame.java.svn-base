    private CheckNode parseJar(final File srcFile) {
        //open as zip file
        ZipFile testFile;
        try {
            testFile = new ZipFile(srcFile);
        } catch (ZipException ex) {
            ex.printStackTrace();
            return null;
        } catch (IOException ex) {
            ex.printStackTrace();
            return null;
        }
        
        //create root (archive) node
        CheckNode root=new CheckNode(new NodeEntry() {
            public InputStream getInputStream() {
                return null;
            }
            @Override
            public String toString() {
                String name=srcFile.getName();
                return name.substring(0, name.lastIndexOf("."));
            }
            
            public Icon getIcon() {
                return Resources.ARCHIVE;
            }
            
            public String getPathname() {
                return ProfileManager.getTestPathname(srcFile);
            }
            
            public NodeEntry.TestType getType() {
                return null;
            }
        }, true, false);
        
        //create one node for each file in archive
        for (Enumeration<? extends ZipEntry> en = testFile.entries(); en.hasMoreElements();) {
            ZipEntry zentry = en.nextElement();
            if(zentry.isDirectory()) continue;
            
            final String zEntryName=zentry.getName();
            NodeEntry entry=new NodeEntry() {
                public InputStream getInputStream() {
                    try {
                        ZipFile zfile=new ZipFile(srcFile);
                        return zfile.getInputStream(zfile.getEntry(zEntryName));
                    } catch (ZipException ex) {
                        ex.printStackTrace();
                    } catch (IOException ex) {
                        ex.printStackTrace();
                    }
                    return null;
                }
                
                @Override
                public String toString() {
                    return zEntryName;
                }
                
                public Icon getIcon() {
                    return null;
                }
                
                public NodeEntry.TestType getType() {
                    return srcFile.getName().endsWith(".xml") ? NodeEntry.TestType.XML : NodeEntry.TestType.TESTv1;
                    //TODO : bug asi
                }
                
                public String getPathname() {
                    return ProfileManager.getTestPathname(srcFile)+zEntryName;
                }
            };
            
            root.add(new CheckNode(entry, false, false));
        }
        
        return root;
    }
    
    private CheckNode parseTestDic(File dic) {
        assert dic.isDirectory();
        CheckNode root=new CheckNode(dic.getName(), true, false);
        
        for (final File file : dic.listFiles()) {
            if (file.isHidden()) continue; //Ignore hidden files
            if (file.getName().startsWith(".")) continue; //Ignore hidden files
            
            if(file.isDirectory()) {
                root.add(parseTestDic(file));
            } else if(file.getName().endsWith(".jar")) {
                root.add(parseJar(file));
            } else {
                NodeEntry entry=new NodeEntry() {
                    public InputStream getInputStream() {
                        try {
                            return new FileInputStream(file);
                        } catch (FileNotFoundException ex) {
                            ex.printStackTrace();
                        }
                        return null;
                    }
                    @Override
                    public String toString() {
                        return file.getName();
                    }
                    
                    public Icon getIcon() {
                        return null;
                    }
                    
                    public NodeEntry.TestType getType() {
                        return file.getName().endsWith(".xml") ? NodeEntry.TestType.XML : NodeEntry.TestType.TESTv1;
                    }
                    
                    public String getPathname() {
                        return ProfileManager.getTestPathname(file);
                    }
                };
                
                root.add(new CheckNode(entry, false, false));
            }
        }
        return root;
    }
    
    private File getStartFolder() {
        File dictDir=Settings.INSTANCE.getDictDir();
        
        if(dictDir==null) {
            File startFolder=new File("../slovniky");
            if(!startFolder.exists()) startFolder=new File("slovniky");
            if(!startFolder.exists()) {
                boolean result=startFolder.mkdir();
                if(result)
                    JOptionPane.showMessageDialog(this, "Adresáø slovníky nebyl nalezen; byl vytvoøen.", "Adresáø slovníky nenalezen", JOptionPane.INFORMATION_MESSAGE);
                else {
                    JOptionPane.showMessageDialog(this, "Adresáø slovníky nebyl nalezen, ani se ho nepodaøilo vytvoøit.", "Adresáø slovníky nenalezen", JOptionPane.ERROR_MESSAGE);
                    return null;
                }
            }
            
            dictDir=startFolder;
            Settings.INSTANCE.setDictDir(dictDir);
        }
        
        return dictDir;
    }
    
    private void initStatsDir() {
        if(Settings.INSTANCE.getStatsDir()==null) {
            File statsDir=new File(getStartFolder().getParent(), "stats");
            if(!statsDir.exists()) {
                boolean result=statsDir.mkdir();
                if(result)
                    JOptionPane.showMessageDialog(this, "Adresáø pro statistky nebyl nalezen; byl vytvoøen.", "Adresáø slovníky nenalezen", JOptionPane.INFORMATION_MESSAGE);
                else {
                    JOptionPane.showMessageDialog(this, "Adresáø pro statistky nebyl nalezen, ani se ho nepodaøilo vytvoøit.", "Adresáø slovníky nenalezen", JOptionPane.ERROR_MESSAGE);
                }
            }
            
            if(statsDir.exists())
                Settings.INSTANCE.setStatsDir(statsDir);
        }
    }
    
    private CheckNode createRootNode() {
        return parseTestDic(getStartFolder());
    }
    
